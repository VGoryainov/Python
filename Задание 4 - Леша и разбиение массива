n = int(input())
a = list(map(int, input().split()))
if all(x == 0 for x in a):
    print("NO")
else:
    print("YES")
    total = sum(a)
    if total != 0:
        print(1)
        print(1, n)
    else:
        for i in range(n):
            if a[i] != 0:
                pos = i
                break
        print(2)
        print(1, pos + 1)
        print(pos + 2, n)
